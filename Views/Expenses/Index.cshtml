<h1 class="d-flex justify-content-center my-5">Expenses for @ViewData["month"] @ViewData["year"]</h1>
<div class="container ">
    <div id="createEditExpensesForm"  class=" bg-primary rounded-5" style="z-index: 1; position: fixed;" hidden>
        <!--
            <partial name="./Partial/createEditExpensesForm.cshtml" />
        -->
        @await Html.PartialAsync("./Partial/createEditExpensesForm.cshtml",ViewData["month"])
    </div>
    <div class="row">
        <h2 class="col-4 "><span class="align-middle">Week 1</span></h2>
        <button id="createEditExpensesFormButton" class="btn btn-outline-light my-3 text-center col-8">New Expense <i
                class="bi bi-plus-circle"></i></button>
    </div>
    <partial name="./Partial/expensesTables.cshtml" />
</div>

<!--SCRIPTS-->
<partial name="./Scripts/IndexTableLoad" />