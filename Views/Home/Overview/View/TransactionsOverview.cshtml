@model MasterOverviewList;

<div>

 <partial name="./ChangeYear.cshtml" />

    <table id="Overview_table" class="table table-striped">
        <thead>
            <tr>
                <td >Month</td>
                <td >Week 1</td>
                <td >Week 2</td>
                <td >Week 3</td>
                <td >Week 4</td>
                <td >Week 5</td>
                <td >Total Expenses</td>
                <td >Total Fixed Expenses</td>
                <td >Total Income</td>
                <td >Difference</td>
            </tr>
        </thead>
        <tbody>
                 @foreach(var monthlyTransaction in Model.monthlyTransactions){
                    <tr>
                        <td >@monthlyTransaction.getMonth()</td>
                        @foreach(var weeklyTransactions in monthlyTransaction.getWeeklyTransactions()){
                            <td > @weeklyTransactions</td>
                        }
                        <td > @monthlyTransaction.getTotalWeeklyTransactions()</td>
                        <td > @monthlyTransaction.getFixedExpenses()</td>
                        <td > @monthlyTransaction.getIncomeExpenses()</td>
                        <td > @monthlyTransaction.getDifference()</td>
                    </tr>
                }
                
                <partial name="./yearlyTotals.cshtml" />
           
        </tbody>
    </table>


</div>

<partial name="../Script/LoadOverviewTable.cshtml"/>

<script>
     OverviewTableLoader.initialize()
</script>